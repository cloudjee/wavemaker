CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE DEPARTMENT(DEPTID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),BUDGET INTEGER,Q1 INTEGER,Q2 INTEGER,Q3 INTEGER,Q4 INTEGER,DEPTCODE VARCHAR(20),LOCATION VARCHAR(255),TENANTID INTEGER DEFAULT 1)
CREATE MEMORY TABLE EMPLOYEE(EID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255),STREET VARCHAR(255),CITY VARCHAR(255),STATE VARCHAR(2),ZIP VARCHAR(255),BIRTHDATE DATE,PICURL VARCHAR(255),TWITTERID VARCHAR(40),DEPTID INTEGER,MANAGERID INTEGER,TENANTID INTEGER DEFAULT 1,CONSTRAINT DEPTFKEY FOREIGN KEY(DEPTID) REFERENCES DEPARTMENT(DEPTID),CONSTRAINT MGRFKEY FOREIGN KEY(MANAGERID) REFERENCES EMPLOYEE(EID))
CREATE MEMORY TABLE VACATION(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,STARTDATE DATE,ENDDATE DATE,EMPID INTEGER NOT NULL,TENANTID INTEGER DEFAULT 1,CONSTRAINT EMPFKEY FOREIGN KEY(EMPID) REFERENCES EMPLOYEE(EID))
CREATE MEMORY TABLE USER(USERID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERNAME VARCHAR(20),PASSWORD VARCHAR(20),ROLE VARCHAR(20),TENANTID INTEGER DEFAULT 1)
ALTER TABLE DEPARTMENT ALTER COLUMN DEPTID RESTART WITH 11
ALTER TABLE EMPLOYEE ALTER COLUMN EID RESTART WITH 11
ALTER TABLE VACATION ALTER COLUMN ID RESTART WITH 22
ALTER TABLE USER ALTER COLUMN USERID RESTART WITH 6
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
